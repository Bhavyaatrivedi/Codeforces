
#include <bits/stdc++.h>
using namespace std;
#define int long long int


signed  main() {

    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int t=1;
    cin>>t;
    while(t--){
        int n,m;
        cin>>n>>m;

        int x,y;
        x=y=0;

        for(int i=0; i<n; i++){
            int a;
            cin>>a;
            x+=a;
        }

       for(int i=0; i<m; i++){
            int a;
            cin>>a;
            y+=a;
        }

        if(x>y){
            cout<<" Tsondu"<<endl;
        }
        if(y>x){
            cout<<"Tenzing "<<endl;
        }
        else{
            cout<<"draw"<<endl;
        }
    }
   
   return 0;
}
