
#include <bits/stdc++.h>
using namespace std;
#define int long long int


signed  main() {

    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int t=1;
    cin>>t;
    while(t--){
        int n;
        cin>>n;

       int a[n], b[n];
       for(int i=0;i<n;i++){
           cin>>a[i];
           b[i] = a[i];
       }

       sort(a, a+n);
        int flag=0;
       for(int i=0;i<n;i++){
           if(a[i] != b[i]){
               flag=1;
               break;
           }
       }

       if(flag) cout<<"Yes"<<endl;
       else cout<<"No"<<endl;
       }
    
   
   return 0;
}
