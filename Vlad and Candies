
#include <bits/stdc++.h>
using namespace std;
#define int long long int


signed  main() {

    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int t=1;
    cin>>t;
    while(t--){
        int n;
        cin>>n;

       int a[n];
       for(int i=0; i<n; i++){
           cin>>a[i];
       }

       if(n == 1){
           if(a[0] ==1) cout<<"Yes";
           else{
               cout<<"No";
               continue;
           }
       }

      sort(a, a+n, greater<int>());
       else{
           if(a[0] - a[1] <=1) cout<<"Yes";
           else cout<<"No"
       }
    }
   
   return 0;
}
