
#include <bits/stdc++.h>
using namespace std;
#define int long long int


signed  main() {
   int t;
   cin>>t;
   while(t--){
     int flag=1;
     string s;
     cin>>s;

    int cnt=0;
     while(s[i] == s[i+1] && i+1<s.size()){
         i++;
         cnt++;
     }

     if(cnt <2){
         flag= 0;
         break;
     }
     if(flag) cout<<"Yes"<<endl;
