
#include <bits/stdc++.h>
using namespace std;
#define int long long int


signed  main() {
   int t;
   cin>>t;
   while(t--){
      int n,k;
      cin>>n>>k;
     
      int a[n];
      int ans=0;
      for(int i=0;i<n;i++){
        cin>>a[i];
      }
      for(int i=0;i<k;i++){
        int cnt=0;
        for(int j=0;j<n;j++){
          if(a[j] & (1<<i){ cnt++};
        }
        if(cnt > n-cnt) ans+= (1<<i);
      }
      cout<<ans<<endl;
   }
   
   return 0;
}
