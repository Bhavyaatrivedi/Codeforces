
#include <bits/stdc++.h>
using namespace std;
#define int long long int


signed  main() {

    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int t=1;
    cin>>t;
    while(t--){
        int n;
        cin>>n;

      if(n > 19) cout<<"No"<<endl;

      else{
          cout<<"Yes"<<endl;
          int num=1;
          cout<<num<<" ";
          while(n--){
              num = num*3;
              cout<<num<<" ";
          }
          cout<<endl;
      }
    }
   
   return 0;
}
