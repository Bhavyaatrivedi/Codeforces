
#include <bits/stdc++.h>
using namespace std;
#define int long long int


signed  main() {
   int t;
   cin>>t;
   while(t--){
      int n;
      cin>>n
     
      int a[n];
      int ans=0;
      for(int i=0;i<n;i++){
        cin>>a[i];
      }
      
      for(int j=30; j>=0; j--){
          int cnt=0;
          for(int i=0; i<n; i++){
              if(a[i] & (1<<j)) cnt++:
          }
          cnt= n-cnt;
          if(cnt <=k) {
              ans+= (1<<j);
              k-= cnt;
          }
      }
      cout<<ans<<endl;
   }
   
   return 0;
}
