
#include <bits/stdc++.h>
using namespace std;
#define int long long int


signed  main() {
   int t;
   cin>>t;
   while(t--){
      int n;
      cin>>n;
     
      string s;
      cin>>s;

      if(s.size() <2){
          cout<<"Yes"<<endl;
      }

      if(s.size() ==2){

          if(s=="00" || s="11") cout<<"No"<<endl;
          cout<<"Yes"<<endl;
      }

      else cout<<"No"<<endl;
   }
   
   return 0;
}
