#define int long long int


signed  main() {
   ll t;
   cin>>t;
   while(t--){
      ll n;
      cin>>n;
      ll sum=0;
      
      for(ll i=0;i<n;i++){
          ll x;
          xin>>x;
          sum+= x-1;
      }
      if(sum&1){
          cout<<"errorgorn"<<endl;
